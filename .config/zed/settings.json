{
  "agent": {
    "model_parameters": [],
    "default_model": {
      "provider": "copilot_chat",
      "model": "claude-sonnet-4"
    },
    "version": "2"
  },
  "language_models": {
    "copilot_chat": {
      "api_url": "https://api.githubcopilot.com/chat/completions",
      "auth_url": "https://api.github.com/copilot_internal/v2/token",
      "models_url": "https://api.githubcopilot.com/models"
    }
  },
  "vim_mode": true,
  "ui_font_size": 16,
  "buffer_font_size": 16,
  "buffer_font_family": "Monaspace Neon Var",
  "theme": {
    "mode": "system",
    "light": "Alucard (Dracula Light)",
    "dark": "Dracula"
  }
}
